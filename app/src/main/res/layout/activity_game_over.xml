<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/competition_primary"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.smk.iwomen.GameOverActivity" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/vertical_margin_1"
        android:text="Game"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/competition_text"
        android:textSize="@dimen/txt_size_header_2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/vertical_margin_2"
        android:text="Time Out !"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/competition_text"
        android:textSize="@dimen/txt_size_header_1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/vertical_margin_2"
        android:gravity="center"
        android:text="We will choose correct answer."
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/competition_text" />

</LinearLayout>
